{
  "name": "circle-plus",
  "version": "0.0.1",
  "scripts": {
    "client:start": "ng serve -o",
    "server:start": "npx nodemon --exec npx ts-node --project src/server/tsconfig.json ./src/server/index.ts",
    "prepare": "node build-scripts/prepare.js && npx webdriver-manager update",
    "test": "npm run client:test && npm run server:test",
    "build": "npm run client:build & npm run server:build",
    "client:build": "ng build --prod",
    "client:test": "ng test",
    "client:lint": "ng lint",
    "server:build": "npx tsc --project src/server/tsconfig.json && npm run server:copy",
    "server:copy": "npx ncp ./package.json ./dist/server/package.json && npx ncp ./src/server/env/example.env ./dist/server/env/example.env",
    "server:test": "cross-env TS_NODE_PROJECT=src/server/tsconfig.spec.json nyc mocha --opts src/server/mocha.opts",
    "server:coverage": "nyc report --reporter=text-lcov | coveralls",
    "e2e": "ng e2e",
    "build:docker": "npm run build && node build-scripts/create-docker-image"
  },
  "nyc": {
    "lines": 0,
    "functions": 0,
    "branches": 0
  },
  "private": true,
